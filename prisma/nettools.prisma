generator client {
  provider = "prisma-client-js"
  output   = "../src/database/generated/nettools"
}

datasource db {
  url      = env("DATABASE_URL_NETTOOLS")
  provider = "postgresql"
}

model Mac_Addresses {
  id     Int     @id @default(autoincrement())
  mac    String  @unique
  vendor String?
}
